{
    "version": 2,
    "builds": [
      {
        "src": "app/src/server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "app/src/api.js"
      },
      {
        "src": "/host/(.*)",
        "dest": "app/src/host.js"
      },
      {
        "src": "/logs/(.*)",
        "dest": "app/src/logs.js"
      },
      {
        "src": "/xss/(.*)",
        "dest": "app/src/xss.js"
      },
      {
        "src": "/(.*)",
        "dest": "app/src/server.js"
      }
    ]
  }
  